\u1085\'ed\u1099\'fb\u1084\'ec \u1090\'f2\u1086\'ee\u1088\'f0\u1075\'e3\u1086\'ee\u1074\'e2\u1099\'fb\u1084\'ec \u1080\'e8\u1083\'eb\u1080\'e8 \u1087\'ef\u1088\'f0\u1086\'ee\u1084\'ec\u1099\'fb\u1096\'f8\u1083\'eb\u1077\'e5
\u1085\'ed\u1085\'ed\u1099\'fb\u1084\'ec \u1094\'f6\u1077\'e5\u1085\'ed\u1090\'f2\u1088\'f0\u1072\'e0\u1084\'ec, \u1087\'ef\u1077\'e5\u1088\'f0\u1077\'e5\u1089\'f1\u1082\'ea\u1072\'e0\u1082\'ea\u1080\'e8\u1074\'e2\u1072\'e0
\u1103\'ff \u1095\'f7\u1077\'e5\u1088\'f0\u1077\'e5\u1079\'e7 \u1080\'e8\u1077\'e5\u1088\'f0\u1072\'e0\u1088\'f0\u1093\'f5\u1080\'e8\u1095\'f7\u1077\'e5\u1089\'f1\u1082\'ea\u1091\'f3\u1102\'fe \u1083\'eb\u1077\'e5\u1089\'f1
\u1090\'f2\u1085\'ed\u1080\'e8\u1094\'f6\u1091\'f3 \u1087\'ef\u1088\'f0\u1080\'e8\u1093\'f5\u1086\'ee\u1076\'e4\u1089\'f1\u1082\'ea\u1080\'e8\u1093\'f5, \u1086\'ee\u1082\'ea\u1088\'f0\u1091\'f3\u1078\'e6\u1085\'ed\u1099\'fb
\u1093\'f5 \u1080\'e8 \u1082\'ea\u1088\'f0\u1072\'e0\u1077\'e5\u1074\'e2\u1099\'fb\u1093\'f5 \u1075\'e3\u1086\'ee\u1088\'f0\u1086\'ee\u1076\'e4\u1086\'ee\u1074\'e2. \u1050\'ca\u1072\'e0\u1078\'e6\u1077\'e5\u1090\'f2\u1089\'f1
\u1103\'ff, \u1073\'e1\u1091\'f3\u1076\'e4\u1090\'f2\u1086\'ee \u1089\'f1\u1077\'e5\u1081\'e9\u1095\'f7\u1072\'e0\u1089\'f1 \u1087\'ef\u1086\'ee\u1089\'f1\u1090\'f2\u1072\'e0\u1074\'e2\u1083\'eb\u1077\'e5\u1085\'ed\u1086\'ee
 \u1085\'ed\u1072\'e0 \u1082\'ea\u1072\'e0\u1088\'f0\u1090\'f2\u1091\'f3 \u1089\'f1\u1072\'e0\u1084\'ec\u1086\'ee \u1080\'e8\u1093\'f5 \u1089\'f1\u1091\'f3\u1097\'f9\u1077\'e5\u1089\'f1\u1090\'f2\u1074\'e2\u1086\'ee\u1074\'e2
\u1072\'e0\u1085\'ed\u1080\'e8\u1077\'e5. \u1057\'d1\u1077\'e5\u1090\'f2\u1086\'ee\u1074\'e2\u1072\'e0\u1085\'ed\u1080\'e8\u1103\'ff \u1085\'ed\u1072\'e0 \u1084\'ec\u1072\'e0\u1083\'eb\u1099\'fb\u1077\'e5 \u1084\'ec\u1072\'e0
\u1089\'f1\u1096\'f8\u1090\'f2\u1072\'e0\u1073\'e1\u1099\'fb \u1085\'ed\u1077\'e5 \u1095\'f7\u1090\'f2\u1086\'ee \u1080\'e8\u1085\'ed\u1086\'ee\u1077\'e5, \u1082\'ea\u1072\'e0\u1082\'ea \u1086\'ee\u1073\'e1\u1099\'fb\u1074\'e2
\u1072\'e0\u1090\'f2\u1077\'e5\u1083\'eb\u1100\'fc\u1089\'f1\u1082\'ea\u1086\'ee\u1077\'e5 \u1087\'efop\u1072\'e0\u1078\'e6\u1077\'e5\u1085\'ed\u1095\'f7\u1077\'e5\u1089\'f1\u1090\'f2\u1074\'e2\u1086\'ee; \u1088\'f0\u1086\'ee
\u1087\'ef\u1086\'ee\u1090\'f2 \u1073\'e1\u1072\'e0\u1085\'ed\u1082\'ea\u1088\'f0\u1086\'ee\u1090\'f2\u1072\'e0, \u1090\'f2\u1077\'e5\u1088\'f0\u1103\'ff\u1102\'fe\u1097\'f9\u1077\'e5\u1075\'e3\u1086\'ee \u1082\'ea\u1083\'eb
\u1080\'e8\u1077\'e5\u1085\'ed\u1090\'f2\u1091\'f3\u1088\'f0\u1091\'f3 \u1080\'e8 \u1085\'ed\u1077\'e5\u1089\'f1\u1087\'ef\u1086\'ee\u1089\'f1\u1086\'ee\u1073\'e1\u1085\'ed\u1086\'ee\u1075\'e3\u1086\'ee \u1089\'f1\u1076\'e4
\u1077\'e5\u1083\'eb\u1072\'e0\u1090\'f2\u1100\'fc \u1080\'e8\u1079\'e7 \u1101\'fd\u1090\'f2\u1086\'ee\u1075\'e3\u1086\'ee \u1085\'ed\u1080\'e8\u1082\'ea\u1072\'e0\u1082\'ea\u1080\'e8\u1093\'f5 \u1087\'ef\u1086\'ee\u1091\'f3
\u1095\'f7\u1080\'e8\u1090\'f2\u1077\'e5\u1083\'eb\u1100\'fc\u1085\'ed\u1099\'fb\u1093\'f5 \u1074\'e2\u1099\'fb\u1074\'e2\u1086\'ee\u1076\'e4\u1086\'ee\u1074\'e2. \u1051\'cb\u1102\'fe\u1076\'e4\u1080\'e8 \u1080\'e8\u1079\'e7
 \u1087\'ef\u1088\'f0\u1086\'ee\u1074\'e2\u1080\'e8\u1085\'ed\u1094\'f6\u1080\'e8\u1072\'e0\u1083\'eb\u1100\'fc\u1085\'ed\u1099\'fb\u1093\'f5 \u1075\'e3\u1086\'ee\u1088\'f0\u1086\'ee\u1076\'e4\u1086\'ee\u1074\'e2 \u1089\'f1
\u1090\'f2\u1072\'e0\u1085\'ed\u1086\'ee\u1074\'e2\u1103\'ff\u1090\'f2\u1089\'f1\u1103\'ff \u1089\'f1\u1083\'eb\u1080\'e8\u1096\'f8\u1082\'ea\u1086\'ee\u1084\'ec \u1084\'ec\u1072\'e0\u1083\'eb\u1086\'ee\u1079\'e7\u1085\'ed
\u1072\'e0\u1095\'f7\u1080\'e8\u1090\'f2\u1077\'e5\u1083\'eb\u1100\'fc\u1085\'ed\u1099\'fb\u1084\'ec\u1080\'e8; \u1087\'ef\u1086\'ee\u1101\'fd\u1090\'f2\u1086\'ee\u1084\'ec\u1091\'f3 \u1086\'ee\u1085\'ed\u1080\'e8 \u1085\'ed
\u1072\'e0\u1095\'f7\u1080\'e8\u1085\'ed\u1072\'e0\u1102\'fe\u1090\'f2 \u1075\'e3\u1086\'ee\u1074\'e2\u1086\'ee\u1088\'f0\u1080\'e8\u1090\'f2\u1100\'fc \u1086\'ee \u1084\'ec\u1072\'e0\u1083\'eb\u1099\'fb\u1093\'f5 \u1084\'ec
\u1072\'e0\u1089\'f1\u1096\'f8\u1090\'f2\u1072\'e0\u1073\'e1\u1072\'e0\u1093\'f5 \u1074\'e2\u1084\'ec\u1077\'e5\u1089\'f1\u1090\'f2\u1086\'ee \u1090\'f2\u1086\'ee\u1075\'e3\u1086\'ee, \u1095\'f7\u1090\'f2\u1086\'ee\u1073\'e1
\u1099\'fb \u1089\'f1\u1093\'f5\u1074\'e2\u1072\'e0\u1090\'f2\u1080\'e8\u1090\'f2\u1100\'fc\u1089\'f1\u1103\'ff \u1079\'e7\u1072\'e0 \u1075\'e3\u1086\'ee\u1083\'eb\u1086\'ee\u1074\'e2\u1091\'f3. \u1045\'c5\u1089\'f1\u1083\'eb
\u1080\'e8 \u1084\'ec\u1077\'e5\u1089\'f1\u1090\'f2\u1085\'ed\u1099\'fb\u1081\'e9 \u1087\'ef\u1088\'f0\u1080\'e8\u1093\'f5\u1086\'ee\u1076\'e4\u1089\'f1\u1082\'ea\u1080\'e8\u1081\'e9 \u1089\'f1\u1074\'e2\u1103\'ff\u1097\'f9
\u1077\'e5\u1085\'ed\u1085\'ed\u1080\'e8\u1082\'ea \u1087\'ef\u1077\'e5\u1088\'f0\u1077\'e5\u1089\'f1\u1090\'f2\u1072\'e0\u1077\'e5\u1090\'f2 \u1073\'e1\u1099\'fb\u1090\'f2\u1100\'fc \u1087\'ef\u1088\'f0\u1080\'e8\u1090\'f2
\u1103\'ff\u1075\'e3\u1072\'e0\u1090\'f2\u1077\'e5\u1083\'eb\u1100\'fc\u1085\'ed\u1099\'fb\u1084\'ec \u1094\'f6\u1077\'e5\u1085\'ed\u1090\'f2\u1088\'f0\u1086\'ee\u1084\'ec \u1087\'ef\u1088\'f0\u1080\'e8\u1093\'f5\u1086\'ee
\u1076\'e4\u1072\'e0, \u1077\'e5\u1084\'ec\u1091\'f3 \u1085\'ed\u1072\'e0\u1076\'e4\u1086\'ee \u1087\'ef\u1088\'f0\u1080\'e8\u1085\'ed\u1080\'e8\u1084\'ec\u1072\'e0\u1090\'f2\u1100\'fc\u1089\'f1\u1103\'ff \u1079\'e7\u1072\'e0
 \u1076\'e4\u1077\'e5\u1083\'eb\u1086\'ee \u1089\'f1 \u1082\'ea\u1072\'e0\u1082\'ea\u1086\'ee\u1075\'e3\u1086\'ee-\u1090\'f2\u1086\'ee \u1076\'e4\u1088\'f0\u1091\'f3\u1075\'e3\u1086\'ee\u1075\'e3\u1086\'ee \u1082\'ea\u1086\'ee
\u1085\'ed\u1094\'f6\u1072\'e0; \u1082\'ea\u1072\'e0\u1078\'e6\u1076\'e4\u1099\'fb\u1081\'e9 \u1075\'e3\u1086\'ee\u1088\'f0\u1086\'ee\u1076\'e4 \u1080\'e8 \u1075\'e3\u1086\'ee\u1088\'f0\u1086\'ee\u1076\'e4\u1086\'ee\u1082\'ea
 \u1078\'e6\u1080\'e8\u1074\'e2\u1077\'e5\u1090\'f2 \u1089\'f1\u1074\'e2\u1086\'ee\u1077\'e5\u1081\'e9 \u1086\'ee\u1082\'ea\u1088\'f0\u1091\'f3\u1075\'e3\u1086\'ee\u1081\'e9; \u1074\'e2\u1086\'ee\u1090\'f2 \u1087\'ef\u1086\'ee
\u1095\'f7\u1077\'e5\u1084\'ec\u1091\'f3 \u1080\'e8\u1084\'ec \u1087\'ef\u1088\'f0\u1077\'e5\u1078\'e6\u1076\'e4\u1077\'e5 \u1074\'e2\u1089\'f1\u1077\'e5\u1075\'e3\u1086\'ee \u1085\'ed\u1072\'e0\u1076\'e4\u1086\'ee \u1079\'e7
\u1072\'e0\u1073\'e1\u1086\'ee\u1090\'f2\u1080\'e8\u1090\'f2\u1100\'fc\u1089\'f1\u1103\'ff \u1086\'ee \u1090\'f2\u1086\'ee\u1084\'ec, \u1095\'f7\u1090\'f2\u1086\'ee\u1073\'e1\u1099\'fb \u1091\'f3\u1076\'e4\u1086\'ee\u1074\'e2
\u1083\'eb\u1077\'e5\u1090\'f2\u1074\'e2\u1086\'ee\u1088\'f0\u1080\'e8\u1090\'f2\u1100\'fc \u1077\'e5\u1077\'e5. \u1069\'dd\u1090\'f2\u1086\'ee \u1087\'ef\u1088\'f0\u1086\'ee\u1073\'e1\u1083\'eb\u1077\'e5\u1084\'ec\u1072\'e0
 \u1085\'ed\u1077\'e5 \u1090\'f2\u1086\'ee\u1083\'eb\u1100\'fc\u1082\'ea\u1086\'ee \u1101\'fd\u1082\'ea\u1086\'ee\u1085\'ed\u1086\'ee\u1084\'ec\u1080\'e8\u1095\'f7\u1077\'e5\u1089\'f1\u1082\'ea\u1072\'e0\u1103\'ff, \u1085\'ed
\u1086\'ee \u1080\'e8 \u1082\'ea\u1091\'f3\u1083\'eb\u1100\'fc\u1090\'f2\u1091\'f3\u1088\'f0\u1085\'ed\u1072\'e0\u1103\'ff \u1080\'e8 \u1086\'ee\u1073\'e1\u1097\'f9\u1077\'e5\u1089\'f1\u1090\'f2\u1074\'e2\u1077\'e5\u1085\'ed
\u1085\'ed\u1072\'e0\u1103\'ff; \u1101\'fd\u1090\'f2\u1086\'ee \u1082\'ea\u1072\'e0\u1082\'ea \u1088\'f0\u1072\'e0\u1079\'e7 \u1090\'f2\u1086\'ee, \u1095\'f7\u1090\'f2\u1086\'ee \u1103\'ff \u1085\'ed\u1072\'e0\u1079\'e7\u1099\'fb
\u1074\'e2\u1072\'e0\u1102\'fe \u1088\'f0\u1077\'e5\u1075\'e3\u1080\'e8\u1086\'ee\u1085\'ed\u1072\'e0\u1083\'eb\u1080\'e8\u1079\'e7\u1084\'ec\u1086\'ee\u1084\'ec.\par
\u1053\'cd\u1077\'e5\u1087\'ef\u1086\'ee\u1089\'f1\u1088\'f0\u1077\'e5\u1076\'e4\u1089\'f1\u1090\'f2\u1074\'e2\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee\u1081\'e9 \u1079\'e7\u1072\'e0\u1076\'e4\u1072\'e0\u1095\'f7\u1077\'e5
\u1081\'e9 \u1082\'ea\u1072\'e0\u1078\'e6\u1076\'e4\u1086\'ee\u1075\'e3\u1086\'ee \u1075\'e3\u1086\'ee\u1088\'f0\u1086\'ee\u1076\'e4\u1072\'e0, \u1086\'ee\u1090\'f2 \u1088\'f0\u1077\'e5\u1096\'f8\u1077\'e5\u1085\'ed\u1080\'e8
\u1103\'ff \u1082\'ea\u1086\'ee\u1090\'f2\u1086\'ee\u1088\'f0\u1086\'ee\u1081\'e9 \u1087\'ef\u1088\'f0\u1103\'ff\u1084\'ec\u1086\'ee \u1079\'e7\u1072\'e0\u1074\'e2\u1080\'e8\u1089\'f1\u1080\'e8\u1090\'f2 \u1077\'e5\u1075\'e3
\u1086\'ee \u1089\'f1\u1091\'f3\u1097\'f9\u1077\'e5\u1089\'f1\u1090\'f2\u1074\'e2\u1086\'ee\u1074\'e2\u1072\'e0\u1085\'ed\u1080\'e8\u1077\'e5, \u1089\'f1\u1090\'f2\u1072\'e0\u1085\'ed\u1086\'ee\u1074\'e2\u1080\'e8\u1090\'f2
\u1089\'f1\u1103\'ff \u1086\'ee\u1074\'e2\u1083\'eb\u1072\'e0\u1076\'e4\u1077\'e5\u1085\'ed\u1080\'e8\u1077\'e5 \u1086\'ee\u1082\'ea\u1088\'f0\u1091\'f3\u1075\'e3\u1086\'ee\u1081\'e9, \u1089\'f1\u1074\'e2\u1086\'ee\u1080\'e8
\u1084\'ec \'ab\u1090\'f2\u1099\'fb\u1083\'eb\u1086\'ee\u1084\'ec\'bb. \u1056\'d0\u1072\'e0\u1085\'ed\u1100\'fc\u1096\'f8\u1077\'e5 \u1086\'ee\u1085\'ed\u1086\'ee \u1086\'ee\u1073\'e1\u1077\'e5\u1089\'f1\u1087\'ef\u1077\'e5
\u1095\'f7\u1080\'e8\u1074\'e2\u1072\'e0\u1083\'eb\u1086\'ee\u1089\'f1\u1100\'fc \u1089\'f1\u1072\'e0\u1084\'ec\u1086\'ee \u1089\'f1\u1086\'ee\u1073\'e1\u1086\'ee\u1081\'e9, \u1085\'ed\u1077\'e5\u1076\'e4\u1086\'ee\u1089\'f1
\u1090\'f2\u1072\'e0\u1090\'f2\u1082\'ea\u1086\'ee\u1084\'ec \u1076\'e4\u1072\'e0\u1083\'eb\u1100\'fc\u1085\'ed\u1080\'e8\u1093\'f5 \u1082\'ea\u1086\'ee\u1084\'ec\u1084\'ec\u1091\'f3\u1085\'ed\u1080\'e8\u1082\'ea\u1072\'e0
\u1094\'f6\u1080\'e8\u1081\'e9; \u1089\'f1\u1087\'ef\u1086\'ee\u1089\'f1\u1086\'ee\u1073\'e1\u1089\'f1\u1090\'f2\u1074\'e2\u1086\'ee\u1074\'e2\u1072\'e0\u1083\'eb\u1080\'e8 \u1101\'fd\u1090\'f2\u1086\'ee\u1084\'ec\u1091\'f3
 \u1080\'e8 \u1094\'f6\u1077\'e5\u1088\'f0\u1082\'ea\u1086\'ee\u1074\'e2\u1100\'fc, \u1080\'e8 \u1074\'e2\u1077\'e5\u1089\'f1\u1100\'fc \u1089\'f1\u1090\'f2\u1088\'f0\u1086\'ee\u1081\'e9 \u1078\'e6\u1080\'e8\u1079\'e7\u1085\'ed
\u1080\'e8. \u1053\'cd\u1099\'fb\u1085\'ed\u1077\'e5 \u1090\'f2\u1072\'e0\u1082\'ea\u1086\'ee\u1077\'e5 \u1077\'e5\u1089\'f1\u1090\'f2\u1077\'e5\u1089\'f1\u1090\'f2\u1074\'e2\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee\u1077\'e5
 \u1087\'ef\u1088\'f0\u1077\'e5\u1086\'ee\u1073\'e1\u1083\'eb\u1072\'e0\u1076\'e4\u1072\'e0\u1085\'ed\u1080\'e8\u1077\'e5 \u1084\'ec\u1077\'e5\u1089\'f1\u1090\'f2\u1085\'ed\u1099\'fb\u1093\'f5 \u1094\'f6\u1077\'e5\u1085\'ed
\u1090\'f2\u1088\'f0\u1086\'ee\u1074\'e2 \u1089\'f1\u1090\'f2\u1088\'f0\u1072\'e0\u1096\'f8\u1085\'ed\u1086\'ee \u1087\'ef\u1086\'ee\u1076\'e4\u1086\'ee\u1088\'f0\u1074\'e2\u1072\'e0\u1085\'ed\u1086\'ee; \u1075\'e3\u1086\'ee
\u1088\'f0\u1086\'ee\u1076\'e4\u1072\'e0 \u1087\'ef\u1088\'f0\u1077\'e5\u1074\'e2\u1088\'f0\u1072\'e0\u1090\'f2\u1080\'e8\u1083\'eb\u1080\'e8\u1089\'f1\u1100\'fc \u1074\'e2 \u1089\'f1\u1090\'f2\u1072\'e0\u1088\'f0\u1077\'e5
\u1102\'fe\u1097\'f9\u1091\'f3\u1102\'fe \u1072\'e0\u1088\'f0\u1080\'e8\u1089\'f1\u1090\'f2\u1086\'ee\u1082\'ea\u1088\'f0\u1072\'e0\u1090\'f2\u1080\'e8\u1102\'fe \u1089\'f1 \u1085\'ed\u1077\'e5\u1089\'f1\u1082\'ea\u1086\'ee
\u1083\'eb\u1100\'fc\u1082\'ea\u1086\'ee \u1087\'ef\u1086\'ee\u1086\'ee\u1073\'e1\u1083\'eb\u1091\'f3\u1087\'ef\u1080\'e8\u1074\'e2\u1096\'f8\u1077\'e5\u1081\'e9\u1089\'f1\u1103\'ff \u1073\'e1\u1099\'fb\u1083\'eb\u1086\'ee
\u1081\'e9 \u1089\'f1\u1083\'eb\u1072\'e0\u1074\'e2\u1086\'ee\u1081\'e9. \u1061\'d5\u1086\'ee\u1088\'f0\u1086\'ee\u1096\'f8\u1086\'ee, \u1084\'ec\u1086\'ee\u1078\'e6\u1085\'ed\u1086\'ee \u1073\'e1\u1099\'fb\u1083\'eb\u1086\'ee
 \u1073\'e1\u1099\'fb \u1087\'ef\u1088\'f0\u1077\'e5\u1076\'e4\u1086\'ee\u1089\'f1\u1090\'f2\u1072\'e0\u1074\'e2\u1080\'e8\u1090\'f2\u1100\'fc \u1080\'e8\u1084\'ec \u1074\'e2\u1099\'fb\u1084\'ec\u1080\'e8\u1088\'f0\u1072\'e0
\u1090\'f2\u1100\'fc \u1082\'ea\u1072\'e0\u1082\'ea \u1085\'ed\u1077\'e5\u1078\'e6\u1080\'e8\u1079\'e7\u1085\'ed\u1077\'e5\u1089\'f1\u1087\'ef\u1086\'ee\u1089\'f1\u1086\'ee\u1073\'e1\u1085\'ed\u1099\'fb\u1084\'ec \u1087\'ef
\u1077\'e5\u1088\'f0\u1077\'e5\u1078\'e6\u1080\'e8\u1090\'f2\u1082\'ea\u1072\'e0\u1084\'ec \u1087\'ef\u1088\'f0\u1086\'ee\u1096\'f8\u1083\'eb\u1086\'ee\u1075\'e3\u1086\'ee; \u1085\'ed\u1086\'ee \u1074\'e2\u1086\'ee\u1090\'f2
 \u1074\'e2\u1086\'ee\u1087\'ef\u1088\'f0\u1086\'ee\u1089\'f1: \u1085\'ed\u1077\'e5 \u1091\'f3\u1090\'f2\u1088\'f0\u1072\'e0\u1095\'f7\u1080\'e8\u1074\'e2\u1072\'e0\u1077\'e5\u1090\'f2\u1089\'f1\u1103\'ff \u1083\'eb\u1080\'e8
 \u1074\'e2\u1089\'f1\u1083\'eb\u1077\'e5\u1076\'e4\u1089\'f1\u1090\'f2\u1074\'e2\u1080\'e8\u1077\'e5 \u1101\'fd\u1090\'f2\u1086\'ee\u1075\'e3\u1086\'ee \u1095\'f7\u1072\'e0\u1089\'f1\u1090\'f2\u1080\'e8\u1094\'f6\u1072\'e0
 \u1079\'e7\u1076\'e4\u1086\'ee\u1088\'f0\u1086\'ee\u1074\'e2\u1086\'ee\u1081\'e9 \u1080\'e8 \u1074\'e2\u1099\'fb\u1089\'f1\u1086\'ee\u1082\'ea\u1086\'ee\u1081\'e9 \u1078\'e6\u1080\'e8\u1079\'e7\u1085\'ed\u1077\'e5\u1085\'ed
\u1085\'ed\u1086\'ee\u1081\'e9 \u1086\'ee\u1088\'f0\u1075\'e3\u1072\'e0\u1085\'ed\u1080\'e8\u1079\'e7\u1072\'e0\u1094\'f6\u1080\'e8\u1080\'e8? \u1044\'c4\u1086\'ee\u1087\'ef\u1091\'f3\u1089\'f1\u1090\'f2\u1080\'e8\u1084\'ec
, \u1085\'ed\u1077\'e5 \u1086\'ee\u1089\'f1\u1090\'f2\u1072\'e0\u1085\'ed\u1077\'e5\u1090\'f2\u1089\'f1\u1103\'ff \u1085\'ed\u1080\'e8\u1095\'f7\u1077\'e5\u1075\'e3\u1086\'ee, \u1082\'ea\u1088\'f0\u1086\'ee\u1084\'ec\u1077\'e5
 \u1073\'e1\u1086\'ee\u1083\'eb\u1100\'fc\u1096\'f8\u1080\'e8\u1093\'f5 \u1087\'ef\u1088\'f0\u1086\'ee\u1084\'ec\u1099\'fb\u1096\'f8\u1083\'eb\u1077\'e5\u1085\'ed\u1085\'ed\u1099\'fb\u1093\'f5 \u1094\'f6\u1077\'e5\u1085\'ed
\u1090\'f2\u1088\'f0\u1086\'ee\u1074\'e2 \u1080\'e8 \u1076\'e4\u1077\'e5\u1088\'f0\u1077\'e5\u1074\'e2\u1085\'ed\u1080\'e8; \u1085\'ed\u1086\'ee \u1085\'ed\u1077\'e5 \u1073\'e1\u1091\'f3\u1076\'e4\u1077\'e5\u1090\'f2 \u1083\'eb
\u1080\'e8 \u1090\'f2\u1072\'e0\u1082\'ea\u1072\'e0\u1103\'ff \u1087\'ef\u1077\'e5\u1088\'f0\u1089\'f1\u1087\'ef\u1077\'e5\u1082\'ea\u1090\'f2\u1080\'e8\u1074\'e2\u1072\'e0 \u1085\'ed\u1077\'e5\u1089\'f1\u1082\'ea\u1086\'ee
\u1083\'eb\u1100\'fc\u1082\'ea\u1086\'ee \u1089\'f1\u1090\'f2\u1088\'f0\u1072\'e0\u1096\'f8\u1085\'ed\u1086\'ee\u1074\'e2\u1072\'e0\u1090\'f2\u1086\'ee\u1081\'e9 \u1074\'e2 \u1082\'ea\u1091\'f3\u1083\'eb\u1100\'fc\u1090\'f2
\u1091\'f3\u1088\'f0\u1085\'ed\u1086\'ee\u1084\'ec \u1080\'e8 \u1086\'ee\u1073\'e1\u1097\'f9\u1077\'e5\u1089\'f1\u1090\'f2\u1074\'e2\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee\u1084\'ec \u1086\'ee\u1090\'f2\u1085\'ed\u1086\'ee
\u1096\'f8\u1077\'e5\u1085\'ed\u1080\'e8\u1080\'e8? \u1045\'c5\u1089\'f1\u1083\'eb\u1080\'e8 \u1101\'fd\u1090\'f2\u1086\'ee \u1089\'f1\u1086\'ee\u1074\'e2\u1088\'f0\u1077\'e5\u1084\'ec\u1077\'e5\u1085\'ed\u1085\'ed\u1099\'fb
\u1081\'e9 \u1091\'f3\u1088\'f0\u1073\'e1\u1072\'e0\u1085\'ed\u1080\'e8\u1079\'e7\u1084\'ec, \u1090\'f2\u1086\'ee \u1103\'ff \u1086\'ee\u1093\'f5\u1086\'ee\u1090\'f2\u1085\'ed\u1077\'e5\u1077\'e5 \u1086\'ee\u1090\'f2\u1076\'e4
\u1072\'e0\u1084\'ec \u1087\'ef\u1088\'f0\u1077\'e5\u1076\'e4\u1087\'ef\u1086\'ee\u1095\'f7\u1090\'f2\u1077\'e5\u1085\'ed\u1080\'e8\u1077\'e5 \u1089\'f1\u1088\'f0\u1077\'e5\u1076\'e4\u1085\'ed\u1077\'e5\u1074\'e2\u1077\'e5
\u1082\'ea\u1086\'ee\u1074\'e2\u1086\'ee\u1081\'e9 \u1086\'ee\u1088\'f0\u1075\'e3\u1072\'e0\u1085\'ed\u1080\'e8\u1079\'e7\u1072\'e0\u1094\'f6\u1080\'e8\u1080\'e8 \u1084\'ec\u1080\'e8\u1088\'f0\u1072\'e0.\par
\u1056\'d0\u1077\'e5\u1075\'e3\u1080\'e8\u1086\'ee\u1085\'ed\u1072\'e0\u1083\'eb\u1080\'e8\u1079\'e7\u1084\'ec, \u1073\'e1\u1086\'ee\u1088\'f0\u1100\'fc\u1073\'e1\u1072\'e0 \u1079\'e7\u1072\'e0 \u1089\'f1\u1086\'ee\u1093\'f5
\u1088\'f0\u1072\'e0\u1085\'ed\u1077\'e5\u1085\'ed\u1080\'e8\u1077\'e5 \u1083\'eb\u1086\'ee\u1082\'ea\u1072\'e0\u1083\'eb\u1100\'fc\u1085\'ed\u1099\'fb\u1093\'f5 \u1094\'f6\u1077\'e5\u1085\'ed\u1090\'f2\u1088\'f0\u1086\'ee
\u1074\'e2 \u1074\'e2\u1077\'e5\u1076\'e4\u1077\'e5\u1090\'f2\u1089\'f1\u1103\'ff \u1085\'ed\u1077\'e5 \u1090\'f2\u1086\'ee\u1083\'eb\u1100\'fc\u1082\'ea\u1086\'ee \u1074\'e2 \u1080\'e8\u1085\'ed\u1090\'f2\u1077\'e5\u1088\'f0
\u1077\'e5\u1089\'f1\u1072\'e0\u1093\'f5 \u1084\'ec\u1077\'e5\u1089\'f1\u1090\'f2\u1085\'ed\u1099\'fb\u1093\'f5 \u1083\'eb\u1072\'e0\u1074\'e2\u1086\'ee\u1095\'f7\u1085\'ed\u1080\'e8\u1082\'ea\u1086\'ee\u1074\'e2 \u1080\'e8
 \u1089\'f1\u1072\'e0\u1087\'ef\u1086\'ee\u1078\'e6\u1085\'ed\u1080\'e8\u1082\'ea\u1086\'ee\u1074\'e2, \u1085\'ed\u1086\'ee \u1090\'f2\u1072\'e0\u1082\'ea\u1078\'e6\u1077\'e5 \u1080\'e8 \u1087\'ef\u1086\'ee\u1076\'e4 \u1092\'f4
\u1083\'eb\u1072\'e0\u1075\'e3\u1086\'ee\u1084\'ec \u1092\'f4\u1086\'ee\u1083\'eb\u1100\'fc\u1082\'ea\u1083\'eb\u1086\'ee\u1088\'f0\u1085\'ed\u1086\'ee\u1075\'e3\u1086\'ee \u1089\'f1\u1074\'e2\u1086\'ee\u1077\'e5\u1086\'ee
\u1073\'e1\u1088\'f0\u1072\'e0\u1079\'e7\u1080\'e8\u1103\'ff; \u1086\'ee\u1076\'e4\u1085\'ed\u1072\'e0\u1082\'ea\u1086\'ee \u1088\'f0\u1077\'e5\u1095\'f7\u1100\'fc \u1080\'e8\u1076\'e4\u1077\'e5\u1090\'f2 \u1086\'ee \u1073\'e1
\u1086\'ee\u1083\'eb\u1100\'fc\u1096\'f8\u1077\'e5\u1084\'ec \u8212\'97 \u1086\'ee\u1073\'e1 \u1086\'ee\u1088\'f0\u1075\'e3\u1072\'e0\u1085\'ed\u1080\'e8\u1079\'e7\u1072\'e0\u1094\'f6\u1080\'e8\u1080\'e8 \u1078\'e6\u1080\'e8
\u1079\'e7\u1085\'ed\u1080\'e8. \u1048\'c8\u1083\'eb\u1080\'e8 \u1076\'e4\u1077\'e5\u1088\'f0\u1077\'e5\u1074\'e2\u1085\'ed\u1103\'ff \u1089\'f1\u1090\'f2\u1072\'e0\u1085\'ed\u1077\'e5\u1090\'f2 \u1092\'f4\u1072\'e0\u1073\'e1
\u1088\'f0\u1080\'e8\u1082\'ea\u1086\'ee\u1081\'e9 \u1079\'e7\u1077\'e5\u1088\'f0\u1085\'ed\u1072\'e0, \u1080\'e8\u1083\'eb\u1080\'e8 \u1086\'ee\u1089\'f1\u1090\'f2\u1072\'e0\u1085\'ed\u1077\'e5\u1090\'f2\u1089\'f1\u1103\'ff
 \u1088\'f0\u1086\'ee\u1076\'e4\u1085\'ed\u1099\'fb\u1084\'ec \u1086\'ee\u1095\'f7\u1072\'e0\u1075\'e3\u1086\'ee\u1084\'ec, \u1074\'e2 \u1082\'ea\u1086\'ee\u1090\'f2\u1086\'ee\u1088\'f0\u1086\'ee\u1084\'ec \u1078\'e6\u1080\'e8
\u1074\'e2\u1091\'f3\u1090\'f2 \u1083\'eb\u1102\'fe\u1076\'e4\u1080\'e8. \u1044\'c4\u1083\'eb\u1103\'ff \u1090\'f2\u1086\'ee\u1075\'e3\u1086\'ee \u1095\'f7\u1090\'f2\u1086\'ee\u1073\'e1\u1099\'fb \u1089\'f1\u1090\'f2\u1072\'e0
\u1090\'f2\u1100\'fc \u1088\'f0\u1086\'ee\u1076\'e4\u1085\'ed\u1099\'fb\u1084\'ec \u1086\'ee\u1095\'f7\u1072\'e0\u1075\'e3\u1086\'ee\u1084\'ec, \u1077\'e5\u1081\'e9 \u1085\'ed\u1091\'f3\u1078\'e6\u1085\'ed\u1086\'ee \u1086\'ee
\u1075\'e3\u1088\'f0\u1072\'e0\u1085\'ed\u1080\'e8\u1095\'f7\u1080\'e8\u1090\'f2\u1100\'fc \u1089\'f1\u1077\'e5\u1073\'e1\u1103\'ff; \u1086\'ee\u1085\'ed\u1072\'e0 \u1076\'e4\u1086\'ee\u1083\'eb\u1078\'e6\u1085\'ed\u1072\'e0
 \u1089\'f1\u1090\'f2\u1072\'e0\u1090\'f2\u1100\'fc \u1089\'f1\u1072\'e0\u1084\'ec\u1086\'ee\u1089\'f1\u1090\'f2\u1086\'ee\u1103\'ff\u1090\'f2\u1077\'e5\u1083\'eb\u1100\'fc\u1085\'ed\u1099\'fb\u1084\'ec \u1082\'ea\u1088\'f0
\u1072\'e0\u1077\'e5\u1084\'ec. \u1045\'c5\u1097\'f9\u1077\'e5 \u1082\'ea\u1072\'e0\u1082\'ea\u1080\'e8\u1084\'ec\u1080\'e8-\u1090\'f2\u1086\'ee \u1085\'ed\u1080\'e8\u1090\'f2\u1103\'ff\u1084\'ec\u1080\'e8 \u1086\'ee\u1085\'ed
\u1072\'e0 \u1089\'f1\u1074\'e2\u1103\'ff\u1079\'e7\u1072\'e0\u1085\'ed\u1072\'e0 \u1089\'f1 \u1084\'ec\u1077\'e5\u1089\'f1\u1090\'f2\u1085\'ed\u1099\'fb\u1084\'ec\u1080\'e8 \u1094\'f6\u1077\'e5\u1085\'ed\u1090\'f2\u1088\'f0
\u1072\'e0\u1084\'ec\u1080\'e8, \u1085\'ed\u1086\'ee \u1077\'e5\u1089\'f1\u1083\'eb\u1080\'e8 \u1101\'fd\u1090\'f2\u1080\'e8 \u1084\'ec\u1077\'e5\u1089\'f1\u1090\'f2\u1085\'ed\u1099\'fb\u1077\'e5 \u1094\'f6\u1077\'e5\u1085\'ed
\u1090\'f2\u1088\'f0\u1099\'fb \u1079\'e7\u1072\'e0\u1093\'f5\u1080\'e8\u1088\'f0\u1077\'e5\u1102\'fe\u1090\'f2 \u1080\'e8 \u1089\'f1\u1090\'f2\u1072\'e0\u1085\'ed\u1091\'f3\u1090\'f2 \u1076\'e4\u1077\'e5\u1081\'e9\u1089\'f1
\u1090\'f2\u1074\'e2\u1080\'e8\u1090\'f2\u1077\'e5\u1083\'eb\u1100\'fc\u1085\'ed\u1086\'ee \u1084\'ec\u1072\'e0\u1083\'eb\u1077\'e5\u1085\'ed\u1100\'fc\u1082\'ea\u1080\'e8\u1084\'ec\u1080\'e8 \u1080\'e8 \u1089\'f1\u1084\'ec
\u1077\'e5\u1088\'f0\u1090\'f2\u1077\'e5\u1083\'eb\u1100\'fc\u1085\'ed\u1086\'ee \u1089\'f1\u1082\'ea\u1091\'f3\u1095\'f7\u1085\'ed\u1099\'fb\u1084\'ec\u1080\'e8, \u1086\'ee\u1082\'ea\u1086\'ee\u1085\'ed\u1095\'f7\u1072\'e0
\u1090\'f2\u1077\'e5\u1083\'eb\u1100\'fc\u1085\'ed\u1086\'ee \u1088\'f0\u1072\'e0\u1089\'f1\u1087\'ef\u1072\'e0\u1076\'e4\u1077\'e5\u1090\'f2\u1089\'f1\u1103\'ff \u1089\'f1\u1090\'f2\u1072\'e0\u1088\'f0\u1077\'e5\u1081\'e9
\u1096\'f8\u1072\'e0\u1103\'ff \u1086\'ee\u1088\'f0\u1075\'e3\u1072\'e0\u1085\'ed\u1080\'e8\u1079\'e7\u1072\'e0\u1094\'f6\u1080\'e8\u1103\'ff \u1085\'ed\u1072\'e0\u1096\'f8\u1077\'e5\u1075\'e3\u1086\'ee \u1084\'ec\u1080\'e8
\u1088\'f0\u1072\'e0. \u1063\'d7\u1090\'f2\u1086\'ee \u1082\'ea\u1072\'e0\u1089\'f1\u1072\'e0\u1077\'e5\u1090\'f2\u1089\'f1\u1103\'ff \u1084\'ec\u1077\'e5\u1085\'ed\u1103\'ff, \u1090\'f2\u1086\'ee \u1103\'ff \u1089\'f1\u1095\'f7
\u1080\'e8\u1090\'f2\u1072\'e0\u1083\'eb \u1073\'e1\u1099\'fb \u1101\'fd\u1090\'f2\u1086\'ee \u1073\'e1\u1086\'ee\u1083\'eb\u1100\'fc\u1096\'f8\u1080\'e8\u1084\'ec \u1073\'e1\u1077\'e5\u1076\'e4\u1089\'f1\u1090\'f2\u1074\'e2
\u1080\'e8\u1077\'e5\u1084\'ec, \u1095\'f7\u1077\'e5\u1084\'ec \u1101\'fd\u1087\'ef\u1080\'e8\u1076\'e4\u1077\'e5\u1084\'ec\u1080\'e8\u1103\'ff \u1095\'f7\u1091\'f3\u1084\'ec\u1099\'fb. \u1056\'d0\u1077\'e5\u1075\'e3\u1080\'e8
\u1086\'ee\u1085\'ed\u1072\'e0\u1083\'eb\u1080\'e8\u1079\'e7\u1084\'ec \u1078\'e6\u1076\'e4\u1077\'e5\u1090\'f2 \u1089\'f1\u1074\'e2\u1086\'ee\u1080\'e8\u1093\'f5 \u1087\'ef\u1086\'ee\u1083\'eb\u1080\'e8\u1090\'f2\u1080\'e8
\u1082\'ea\u1086\'ee\u1074\'e2 \u8212\'97 \u1083\'eb\u1102\'fe\u1076\'e4\u1077\'e5\u1081\'e9, \u1074\'e2\u1086\'ee\u1086\'ee\u1076\'e4\u1091\'f3\u1096\'f8\u1077\'e5\u1074\'e2\u1083\'eb\u1077\'e5\u1085\'ed\u1085\'ed\u1099\'fb
\u1093\'f5 \u1080\'e8\u1076\'e4\u1077\'e5\u1077\'e5\u1081\'e9, \u1095\'f7\u1090\'f2\u1086\'ee \u1041\'c1\u1077\'e5\u1088\'f0\u1086\'ee\u1091\'f3\u1085\'ed \u1076\'e4\u1086\'ee\u1083\'eb\u1078\'e6\u1077\'e5\u1085\'ed \u1089\'f1
\u1090\'f2\u1072\'e0\u1090\'f2\u1100\'fc \u1041\'c1\u1077\'e5\u1088\'f0\u1086\'ee\u1091\'f3\u1085\'ed\u1086\'ee\u1084\'ec, \u1072\'e0 \u1042\'c2\u1099\'fb\u1089\'f1\u1086\'ee\u1082\'ea\u1077\'e5 \u1052\'cc\u1099\'fb\u1090\'f2
\u1086\'ee \u8212\'97 \u1042\'c2\u1099\'fb\u1089\'f1\u1086\'ee\u1082\'ea\u1080\'e8\u1084\'ec \u1052\'cc\u1099\'fb\u1090\'f2\u1086\'ee\u1084\'ec, \u1072\'e0 \u1085\'ed\u1077\'e5 \u1082\'ea\u1072\'e0\u1082\'ea\u1080\'e8\u1084\'ec
\u1080\'e8-\u1090\'f2\u1086\'ee \u1091\'f3\u1073\'e1\u1086\'ee\u1075\'e3\u1080\'e8\u1084\'ec\u1080\'e8 \u1079\'e7\u1072\'e0\u1076\'e4\u1074\'e2\u1086\'ee\u1088\'f0\u1082\'ea\u1072\'e0\u1084\'ec\u1080\'e8, \u1075\'e3\u1076\'e4
\u1077\'e5 \u1084\'ec\u1086\'ee\u1078\'e6\u1085\'ed\u1086\'ee \u1078\'e6\u1080\'e8\u1090\'f2\u1100\'fc \u1083\'eb\u1080\'e8\u1096\'f8\u1100\'fc \u1074\'e2\u1088\'f0\u1077\'e5\u1084\'ec\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee
 \u1080\'e8 \u1075\'e3\u1076\'e4\u1077\'e5 \u1074\'e2\u1089\'f1\u1077\'e5 \u1087\'ef\u1088\'f0\u1080\'e8\u1090\'f2\u1077\'e5\u1088\'f0\u1087\'ef\u1077\'e5\u1083\'eb\u1080\'e8\u1089\'f1\u1100\'fc \u1082\'ea \u1084\'ec\u1072\'e0
\u1083\'eb\u1099\'fb\u1084\'ec \u1084\'ec\u1072\'e0\u1089\'f1\u1096\'f8\u1090\'f2\u1072\'e0\u1073\'e1\u1072\'e0\u1084\'ec.\par
\par
