\u1085\'ed\u1080\'e8\u1077\'e5, \u1095\'f7\u1090\'f2\u1086\'ee \u1074\'e2 \u1090\'f2\u1072\'e0\u1082\'ea\u1080\'e8\u1093\'f5 \u1084\'ec\u1072\'e0\u1083\'eb\u1099\'fb\u1093\'f5 \u1084\'ec\u1072\'e0\u1089\'f1\u1096\'f8\u1090\'f2
\u1072\'e0\u1073\'e1\u1072\'e0\u1093\'f5 \u1086\'ee\u1085\'ed \u1085\'ed\u1077\'e5 \u1073\'e1\u1091\'f3\u1076\'e4\u1077\'e5\u1090\'f2 \u1079\'e7\u1088\'f0\u1103\'ff \u1090\'f2\u1088\'f0\u1072\'e0\u1090\'f2\u1080\'e8\u1090\'f2
\u1100\'fc \u1089\'f1\u1074\'e2\u1086\'ee\u1081\'e9 \u1087\'ef\u1099\'fb\u1083\'eb \u8212\'97 \u1080\'e8 \u1073\'e1\u1072\'e0\u1089\'f1\u1090\'f2\u1072\'e0. \u1042\'c2\u1086\'ee\u1082\'ea\u1088\'f0\u1091\'f3\u1075\'e3 \u1085\'ed
\u1072\'e0\u1089\'f1\u1090\'f2\u1086\'ee\u1103\'ff\u1097\'f9\u1077\'e5\u1075\'e3\u1086\'ee \u1084\'ec\u1091\'f3\u1078\'e6\u1077\'e5\u1089\'f1\u1090\'f2\u1074\'e2\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee\u1075\'e3\u1086\'ee
 \u1084\'ec\u1091\'f3\u1078\'e6\u1095\'f7\u1080\'e8\u1085\'ed\u1099\'fb \u1085\'ed\u1080\'e8\u1082\'ea\u1086\'ee\u1075\'e3\u1076\'e4\u1072\'e0 \u1085\'ed\u1077\'e5 \u1073\'e1\u1099\'fb\u1074\'e2\u1072\'e0\u1077\'e5\u1090\'f2
 \u1084\'ec\u1072\'e0\u1083\'eb\u1099\'fb\u1093\'f5 \u1084\'ec\u1072\'e0\u1089\'f1\u1096\'f8\u1090\'f2\u1072\'e0\u1073\'e1\u1086\'ee\u1074\'e2, \u1085\'ed\u1072\'e0\u1087\'ef\u1088\'f0\u1086\'ee\u1090\'f2\u1080\'e8\u1074\'e2
, \u1084\'ec\u1072\'e0\u1089\'f1\u1096\'f8\u1090\'f2\u1072\'e0\u1073\'e1\u1099\'fb \u1074\'e2\u1086\'ee\u1082\'ea\u1088\'f0\u1091\'f3\u1075\'e3 \u1085\'ed\u1077\'e5\u1075\'e3\u1086\'ee \u1074\'e2\u1089\'f1\u1077\'e5\u1075\'e3
\u1076\'e4\u1072\'e0 \u1074\'e2\u1077\'e5\u1089\'f1\u1100\'fc\u1084\'ec\u1072\'e0 \u1074\'e2\u1085\'ed\u1091\'f3\u1096\'f8\u1080\'e8\u1090\'f2\u1077\'e5\u1083\'eb\u1100\'fc\u1085\'ed\u1099\'fb\u1077\'e5 \u1080\'e8 \u1076\'e4
\u1072\'e0\u1078\'e6\u1077\'e5 \u1086\'ee\u1075\'e3\u1088\'f0\u1086\'ee\u1084\'ec\u1085\'ed\u1099\'fb\u1077\'e5. \u1064\'d8\u1077\'e5\u1082\'ea\u1083\'eb\u1090\'f2\u1086\'ee\u1085\'ed\u1072\'e0 \u1074\'e2\u1087\'ef\u1086\'ee
\u1083\'eb\u1085\'ed\u1077\'e5 \u1091\'f3\u1089\'f1\u1090\'f2\u1088\'f0\u1072\'e0\u1080\'e8\u1074\'e2\u1072\'e0\u1083\'eb \u1070\'de\u1078\'e6\u1085\'ed\u1099\'fb\u1081\'e9 \u1087\'ef\u1086\'ee\u1083\'eb\u1102\'fe\u1089\'f1
, \u1087\'ef\u1086\'ee\u1090\'f2\u1086\'ee\u1084\'ec\u1091\'f3 \u1095\'f7\u1090\'f2\u1086\'ee \u1080\'e8\u1084\'ec\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee \u1070\'de\u1078\'e6\u1085\'ed\u1086\'ee\u1075\'e3\u1086\'ee \u1087\'ef
\u1086\'ee\u1083\'eb\u1102\'fe\u1089\'f1\u1072\'e0 \u1086\'ee\u1085\'ed \u1093\'f5\u1086\'ee\u1090\'f2\u1077\'e5\u1083\'eb \u1076\'e4\u1086\'ee\u1089\'f1\u1090\'f2\u1080\'e8\u1075\'e3\u1085\'ed\u1091\'f3\u1090\'f2\u1100\'fc
; \u1085\'ed\u1086\'ee \u1074\'e2\u1077\'e5\u1095\'f7\u1085\'ed\u1086\'ee \u1085\'ed\u1077\'e5\u1076\'e4\u1086\'ee\u1074\'e2\u1086\'ee\u1083\'eb\u1100\'fc\u1085\'ed\u1099\'fb\u1081\'e9 \u1095\'f7\u1077\'e5\u1093\'f5 \u1087\'ef
\u1086\'ee\u1076\'e4\u1085\'ed\u1103\'ff\u1083\'eb \u1073\'e1\u1099\'fb \u1075\'e3\u1088\'f0\u1086\'ee\u1084\'ec\u1082\'ea\u1080\'e8\u1081\'e9 \u1087\'ef\u1088\'f0\u1086\'ee\u1090\'f2\u1077\'e5\u1089\'f1\u1090\'f2\u1091\'f3
\u1102\'fe\u1097\'f9\u1080\'e8\u1081\'e9 \u1082\'ea\u1088\'f0\u1080\'e8\u1082\'ea, \u1095\'f7\u1090\'f2\u1086\'ee \u1070\'de\u1078\'e6\u1085\'ed\u1099\'fb\u1081\'e9 \u1087\'ef\u1086\'ee\u1083\'eb\u1102\'fe\u1089\'f1 \u1085\'ed
\u1077\'e5 \u1042\'c2\u1072\'e0\u1094\'f6\u1083\'eb\u1072\'e0\u1074\'e2\u1089\'f1\u1082\'ea\u1072\'e0\u1103\'ff \u1087\'ef\u1083\'eb\u1086\'ee\u1097\'f9\u1072\'e0\u1076\'e4\u1100\'fc \u1080\'e8 \u1085\'ed\u1077\'e5 \u1041\'c1
\u1086\'ee\u1083\'eb\u1100\'fc\u1096\'f8\u1072\'e0\u1103\'ff \u1086\'ee\u1087\'ef\u1077\'e5\u1088\'f0\u1072\'e0. \u1048\'c8\u1073\'e1\u1086\'ee \u1087\'ef\u1086\'ee\u1076\'e4\u1086\'ee\u1073\'e1\u1085\'ed\u1099\'fb\u1084\'ec
 \u1078\'e6\u1077\'e5 \u1086\'ee\u1073\'e1\u1088\'f0\u1072\'e0\u1079\'e7\u1086\'ee\u1084\'ec \u1086\'ee\u1085\'ed \u1078\'e6\u1072\'e0\u1083\'eb\u1091\'f3\u1077\'e5\u1090\'f2\u1089\'f1\u1103\'ff, \u1095\'f7\u1090\'f2\u1086\'ee
 \u1041\'c1\u1077\'e5\u1088\'f0\u1086\'ee\u1091\'f3\u1085\'ed \u1085\'ed\u1077\'e5 \u1055\'cf\u1088\'f0\u1072\'e0\u1075\'e3\u1072\'e0 \u1080\'e8 \u1085\'ed\u1077\'e5 \u1056\'d0\u1080\'e8\u1084\'ec. \u1054\'ce\u1085\'ed \u1074\'e2
\u1086\'ee\u1079\'e7\u1084\'ec\u1091\'f3\u1097\'f9\u1077\'e5\u1085\'ed \u1090\'f2\u1077\'e5\u1084\'ec, \u1095\'f7\u1090\'f2\u1086\'ee \u1042\'c2\u1099\'fb\u1089\'f1\u1086\'ee\u1082\'ea\u1077\'e5 \u1052\'cc\u1099\'fb\u1090\'f2
\u1086\'ee \u8212\'97 \u1101\'fd\u1090\'f2\u1086\'ee \u1042\'c2\u1099\'fb\u1089\'f1\u1086\'ee\u1082\'ea\u1077\'e5 \u1052\'cc\u1099\'fb\u1090\'f2\u1086\'ee, \u1072\'e0 \u1085\'ed\u1077\'e5 \u1095\'f7\u1090\'f2\u1086\'ee-\u1085\'ed
\u1080\'e8\u1073\'e1\u1091\'f3\u1076\'e4\u1100\'fc \u1076\'e4\u1088\'f0\u1091\'f3\u1075\'e3\u1086\'ee\u1077\'e5. \u1054\'ce\u1085\'ed \u1087\'ef\u1088\'f0\u1086\'ee\u1090\'f2\u1077\'e5\u1089\'f1\u1090\'f2\u1091\'f3\u1077\'e5
\u1090\'f2 \u1087\'ef\u1088\'f0\u1086\'ee\u1090\'f2\u1080\'e8\u1074\'e2 \u1074\'e2\u1086\'ee\u1079\'e7\u1084\'ec\u1091\'f3\u1090\'f2\u1080\'e8\u1090\'f2\u1077\'e5\u1083\'eb\u1100\'fc\u1085\'ed\u1086\'ee\u1075\'e3\u1086\'ee
 \u1092\'f4\u1072\'e0\u1082\'ea\u1090\'f2\u1072\'e0, \u1095\'f7\u1090\'f2\u1086\'ee \u1074\'e2 \u1050\'ca\u1088\'f0\u1086\'ee\u1084\'ec\u1077\'e5\u1088\'f0\u1078\'e6\u1080\'e8\u1078\'e6\u1077\'e5 \u1086\'ee\u1090\'f2\u1085\'ed
\u1102\'fe\u1076\'e4\u1100\'fc \u1085\'ed\u1077\'e5 \u1087\'ef\u1103\'ff\u1090\'f2\u1100\'fc \u1084\'ec\u1080\'e8\u1083\'eb\u1083\'eb\u1080\'e8\u1086\'ee\u1085\'ed\u1086\'ee\u1074\'e2 \u1078\'e6\u1080\'e8\u1090\'f2\u1077\'e5
\u1083\'eb\u1077\'e5\u1081\'e9 \u1080\'e8 \u1095\'f7\u1090\'f2\u1086\'ee \u1074\'e2 \u1055\'cf\u1088\'f0\u1072\'e0\u1075\'e3\u1077\'e5 \u1085\'ed\u1077\'e5\u1090\'f2 \u1080\'e8\u1085\'ed\u1090\'f2\u1077\'e5\u1083\'eb\u1083\'eb
\u1077\'e5\u1082\'ea\u1090\'f2\u1091\'f3\u1072\'e0\u1083\'eb\u1100\'fc\u1085\'ed\u1086\'ee\u1081\'e9 \u1072\'e0\u1090\'f2\u1084\'ec\u1086\'ee\u1089\'f1\u1092\'f4\u1077\'e5\u1088\'f0\u1099\'fb \u1055\'cf\u1072\'e0\u1088\'f0
\u1080\'e8\u1078\'e6\u1072\'e0. \u1057\'d1\u1080\'e8\u1090\'f2\u1091\'f3\u1072\'e0\u1094\'f6\u1080\'e8\u1103\'ff \u1073\'e1\u1077\'e5\u1079\'e7\u1074\'e2\u1099\'fb\u1093\'f5\u1086\'ee\u1076\'e4\u1085\'ed\u1072\'e0\u1103\'ff
, \u1087\'ef\u1086\'ee\u1089\'f1\u1082\'ea\u1086\'ee\u1083\'eb\u1100\'fc\u1082\'ea\u1091\'f3 \u1090\'f2\u1091\'f3\u1090\'f2 \u1103\'ff\u1074\'e2\u1085\'ed\u1086\'ee \u1085\'ed\u1080\'e8\u1095\'f7\u1077\'e5\u1075\'e3\u1086\'ee
 \u1085\'ed\u1077\'e5\u1083\'eb\u1100\'fc\u1079\'e7\u1103\'ff \u1087\'ef\u1086\'ee\u1076\'e4\u1077\'e5\u1083\'eb\u1072\'e0\u1090\'f2\u1100\'fc.\par
\u1053\'cd\u1086\'ee \u1088\'f0\u1072\'e0\u1079\'e7 \u1091\'f3\u1078\'e6 \u1103\'ff \u1079\'e7\u1072\'e0\u1075\'e3\u1086\'ee\u1074\'e2\u1086\'ee\u1088\'f0\u1080\'e8\u1083\'eb \u1086\'ee \u1050\'ca\u1088\'f0\u1086\'ee\u1084\'ec
\u1077\'e5\u1088\'f0\u1078\'e6\u1080\'e8\u1078\'e6\u1077\'e5\u8230\'85 \u1093\'f5\u1086\'ee\u1090\'f2\u1103\'ff \u1073\'e1\u1099\'fb\u1083\'eb\u1086\'ee \u1073\'e1\u1099\'fb \u1085\'ed\u1072\'e0\u1087\'ef\u1088\'f0\u1072\'e0
\u1089\'f1\u1085\'ed\u1099\'fb\u1084\'ec \u1091\'f3\u1087\'ef\u1086\'ee\u1074\'e2\u1072\'e0\u1085\'ed\u1080\'e8\u1077\'e5\u1084\'ec, \u1095\'f7\u1090\'f2\u1086\'ee \u1050\'ca\u1088\'f0\u1086\'ee\u1084\'ec\u1077\'e5\u1088\'f0
\u1078\'e6\u1080\'e8\u1078\'e6 \u1084\'ec\u1086\'ee\u1078\'e6\u1085\'ed\u1086\'ee \u1087\'ef\u1088\'f0\u1077\'e5\u1074\'e2\u1088\'f0\u1072\'e0\u1090\'f2\u1080\'e8\u1090\'f2\u1100\'fc \u1074\'e2 \u1085\'ed\u1077\'e5\u1073\'e1
\u1077\'e5\u1089\'f1\u1085\'ed\u1099\'fb\u1081\'e9 \u1048\'c8\u1077\'e5\u1088\'f0\u1091\'f3\u1089\'f1\u1072\'e0\u1083\'eb\u1080\'e8\u1084\'ec, \u1085\'ed\u1086\'ee \u1074\'e2 \u1080\'e8\u1079\'e7\u1074\'e2\u1077\'e5\u1089\'f1
\u1090\'f2\u1085\'ed\u1086\'ee\u1081\'e9 \u1084\'ec\u1077\'e5\u1088\'f0\u1077\'e5 \u1080\'e8 \u1085\'ed\u1072\'e0 \u1082\'ea\u1072\'e0\u1082\'ea\u1086\'ee\u1077\'e5-\u1090\'f2\u1086\'ee \u1074\'e2\u1088\'f0\u1077\'e5\u1084\'ec
\u1103\'ff \u1080\'e8\u1079\'e7 \u1085\'ed\u1077\'e5\u1075\'e3\u1086\'ee \u1085\'ed\u1077\'e5\u1082\'ea\u1086\'ee\u1075\'e3\u1076\'e4\u1072\'e0 \u1091\'f3\u1076\'e4\u1072\'e0\u1083\'eb\u1086\'ee\u1089\'f1\u1100\'fc \u1089\'f1
\u1076\'e4\u1077\'e5\u1083\'eb\u1072\'e0\u1090\'f2\u1100\'fc \u1048\'c8\u1077\'e5\u1088\'f0\u1091\'f3\u1089\'f1\u1072\'e0\u1083\'eb\u1080\'e8\u1084\'ec \u1075\'e3\u1072\'e0\u1085\'ed\u1072\'e0\u1094\'f6\u1082\'ea\u1080\'e8
\u1081\'e9. \u1051\'cb\u1080\'e8\u1090\'f2\u1086\'ee\u1084\'ec\u1099\'fb\u1096\'f8\u1083\'eb\u1100\'fc \u1085\'ed\u1077\'e5 \u1084\'ec\u1086\'ee\u1078\'e6\u1077\'e5\u1090\'f2 \u1089\'f1\u1090\'f2\u1072\'e0\u1090\'f2\u1100\'fc
 \u1055\'cf\u1077\'e5\u1088\'f0\u1080\'e8\u1082\'ea\u1083\'eb\u1086\'ee\u1074\'e2\u1099\'fb\u1084\'ec\u1080\'e8 \u1040\'c0\u1092\'f4\u1080\'e8\u1085\'ed\u1072\'e0\u1084\'ec\u1080\'e8, \u1085\'ed\u1086\'ee \u1074\'e2 \u1080\'e8
\u1079\'e7\u1074\'e2\u1077\'e5\u1089\'f1\u1090\'f2\u1085\'ed\u1086\'ee\u1077\'e5 \u1074\'e2\u1088\'f0\u1077\'e5\u1084\'ec\u1103\'ff \u1086\'ee\u1085\'ed \u1073\'e1\u1099\'fb\u1083\'eb \u1074\'e2\u1086\'ee\u1089\'f1\u1090\'f2
\u1086\'ee\u1095\'f7\u1085\'ed\u1086\'ee\u1095\'f7\u1077\'e5\u1096\'f8\u1089\'f1\u1082\'ea\u1080\'e8\u1084\'ec\u1080\'e8 \u1040\'c0\u1092\'f4\u1080\'e8\u1085\'ed\u1072\'e0\u1084\'ec\u1080\'e8. \u1045\'c5\u1089\'f1\u1083\'eb
\u1080\'e8 \u1041\'c1\u1077\'e5\u1088\'f0\u1086\'ee\u1091\'f3\u1085\'ed \u1085\'ed\u1077\'e5 \u1084\'ec\u1086\'ee\u1078\'e6\u1077\'e5\u1090\'f2 \u1089\'f1\u1090\'f2\u1072\'e0\u1090\'f2\u1100\'fc \u1085\'ed\u1080\'e8 \u1055\'cf
\u1088\'f0\u1072\'e0\u1075\'e3\u1086\'ee\u1081\'e9, \u1085\'ed\u1080\'e8 \u1055\'cf\u1072\'e0\u1088\'f0\u1080\'e8\u1078\'e6\u1077\'e5\u1084\'ec, \u1086\'ee\u1085\'ed \u1084\'ec\u1086\'ee\u1078\'e6\u1077\'e5\u1090\'f2 \u1089\'f1
\u1090\'f2\u1072\'e0\u1090\'f2\u1100\'fc \u1093\'f5\u1086\'ee\u1090\'f2\u1103\'ff \u1073\'e1\u1099\'fb \u1073\'e1\u1083\'eb\u1072\'e0\u1075\'e3\u1086\'ee\u1091\'f3\u1089\'f1\u1090\'f2\u1088\'f0\u1086\'ee\u1077\'e5\u1085\'ed
\u1085\'ed\u1099\'fb\u1084\'ec \u1080\'e8 \u1091\'f3\u1076\'e4\u1086\'ee\u1074\'e2\u1083\'eb\u1077\'e5\u1090\'f2\u1074\'e2\u1086\'ee\u1088\'f0\u1103\'ff\u1102\'fe\u1097\'f9\u1080\'e8\u1084\'ec \u1074\'e2\u1089\'f1\u1077\'e5
 \u1089\'f1\u1074\'e2\u1086\'ee\u1080\'e8 \u1087\'ef\u1086\'ee\u1090\'f2\u1088\'f0\u1077\'e5\u1073\'e1\u1085\'ed\u1086\'ee\u1089\'f1\u1090\'f2\u1080\'e8 \u1041\'c1\u1077\'e5\u1088\'f0\u1086\'ee\u1091\'f3\u1085\'ed\u1086\'ee
\u1084\'ec. \u1052\'cc\u1086\'ee\u1078\'e6\u1085\'ed\u1086\'ee \u1076\'e4\u1072\'e0\u1078\'e6\u1077\'e5 \u1076\'e4\u1086\'ee\u1073\'e1\u1080\'e8\u1074\'e2\u1072\'e0\u1090\'f2\u1100\'fc\u1089\'f1\u1103\'ff, \u1095\'f7\u1090\'f2
\u1086\'ee\u1073\'e1\u1099\'fb \u1086\'ee\u1085\'ed \u1089\'f1\u1090\'f2\u1072\'e0\u1083\'eb \u1074\'e2 \u1093\'f5\u1086\'ee\u1088\'f0\u1086\'ee\u1096\'f8\u1077\'e5\u1084\'ec \u1089\'f1\u1084\'ec\u1099\'fb\u1089\'f1\u1083\'eb
\u1077\'e5 \u1089\'f1\u1083\'eb\u1086\'ee\u1074\'e2\u1072\'e0 \u1089\'f1\u1072\'e0\u1084\'ec\u1099\'fb\u1084\'ec \u1095\'f7\u1090\'f2\u1086\'ee \u1085\'ed\u1080\'e8 \u1085\'ed\u1072\'e0 \u1077\'e5\u1089\'f1\u1090\'f2\u1100\'fc
 \u1073\'e1\u1077\'e5\u1088\'f0\u1086\'ee\u1091\'f3\u1085\'ed\u1089\'f1\u1082\'ea\u1080\'e8\u1084\'ec \u1041\'c1\u1077\'e5\u1088\'f0\u1086\'ee\u1091\'f3\u1085\'ed\u1086\'ee\u1084\'ec. \u1044\'c4\u1083\'eb\u1103\'ff \u1101\'fd
\u1090\'f2\u1086\'ee\u1075\'e3\u1086\'ee, \u1088\'f0\u1072\'e0\u1079\'e7\u1091\'f3\u1084\'ec\u1077\'e5\u1077\'e5\u1090\'f2\u1089\'f1\u1103\'ff, \u1085\'ed\u1091\'f3\u1078\'e6\u1085\'ed\u1086\'ee, \u1095\'f7\u1090\'f2\u1086\'ee
\u1073\'e1\u1099\'fb \u1078\'e6\u1080\'e8\u1090\'f2\u1077\'e5\u1083\'eb\u1080\'e8 \u1041\'c1\u1077\'e5\u1088\'f0\u1086\'ee\u1091\'f3\u1085\'ed\u1072\'e0 \u1073\'e1\u1099\'fb\u1083\'eb\u1080\'e8 \u1085\'ed\u1077\'e5\u1088\'f0
\u1072\'e0\u1074\'e2\u1085\'ed\u1086\'ee\u1076\'e4\u1091\'f3\u1096\'f8\u1085\'ed\u1099\'fb \u1080\'e8\u1084\'ec\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee \u1082\'ea \u1089\'f1\u1091\'f3\u1076\'e4\u1100\'fc\u1073\'e1\u1077\'e5
 \u1041\'c1\u1077\'e5\u1088\'f0\u1086\'ee\u1091\'f3\u1085\'ed\u1072\'e0.\par
\u1045\'c5\u1089\'f1\u1083\'eb\u1080\'e8 \u1084\'ec\u1099\'fb \u1086\'ee\u1075\'e3\u1083\'eb\u1103\'ff\u1085\'ed\u1077\'e5\u1084\'ec\u1089\'f1\u1103\'ff \u1085\'ed\u1077\'e5\u1084\'ec\u1085\'ed\u1086\'ee\u1075\'e3\u1086\'ee
 \u1085\'ed\u1072\'e0 \u1080\'e8\u1089\'f1\u1090\'f2\u1086\'ee\u1088\'f0\u1080\'e8\u1102\'fe, \u1090\'f2\u1086\'ee \u1091\'f3\u1074\'e2\u1080\'e8\u1076\'e4\u1080\'e8\u1084\'ec, \u1095\'f7\u1090\'f2\u1086\'ee \u1093\'f5\u1086\'ee
\u1090\'f2\u1103\'ff \u1089\'f1\u1091\'f3\u1097\'f9\u1077\'e5\u1089\'f1\u1090\'f2\u1074\'e2\u1086\'ee\u1074\'e2\u1072\'e0\u1083\'eb\u1080\'e8 \u1074\'e2\u1077\'e5\u1083\'eb\u1080\'e8\u1082\'ea\u1080\'e8\u1077\'e5 \u1082\'ea
\u1091\'f3\u1083\'eb\u1100\'fc\u1090\'f2\u1091\'f3\u1088\'f0\u1099\'fb, \u1074\'e2\u1099\'fb\u1088\'f0\u1086\'ee\u1089\'f1\u1096\'f8\u1080\'e8\u1077\'e5 \u1080\'e8\u1079\'e7 \u1073\'e1\u1086\'ee\u1083\'eb\u1100\'fc\u1096\'f8
\u1080\'e8\u1093\'f5 \u1084\'ec\u1072\'e0\u1089\'f1\u1096\'f8\u1090\'f2\u1072\'e0\u1073\'e1\u1086\'ee\u1074\'e2, \u1082\'ea\u1072\'e0\u1082\'ea, \u1085\'ed\u1072\'e0\u1087\'ef\u1088\'f0\u1080\'e8\u1084\'ec\u1077\'e5\u1088\'f0
, \u1082\'ea\u1091\'f3\u1083\'eb\u1100\'fc\u1090\'f2\u1091\'f3\u1088\'f0\u1072\'e0 \u1088\'f0\u1080\'e8\u1084\'ec\u1089\'f1\u1082\'ea\u1072\'e0\u1103\'ff, \u1085\'ed\u1086\'ee \u1077\'e5\u1097\'f9\u1077\'e5 \u1095\'f7\u1072\'e0
\u1097\'f9\u1077\'e5 \u1074\'e2\u1077\'e5\u1083\'eb\u1080\'e8\u1082\'ea\u1080\'e8\u1077\'e5 \u1082\'ea\u1091\'f3\u1083\'eb\u1100\'fc\u1090\'f2\u1091\'f3\u1088\'f0\u1099\'fb, \u1082\'ea\u1072\'e0\u1082\'ea, \u1085\'ed\u1072\'e0
\u1087\'ef\u1088\'f0\u1080\'e8\u1084\'ec\u1077\'e5\u1088\'f0, \u1075\'e3\u1088\'f0\u1077\'e5\u1095\'f7\u1077\'e5\u1089\'f1\u1082\'ea\u1072\'e0\u1103\'ff, \u1074\'e2\u1099\'fb\u1088\'f0\u1072\'e0\u1089\'f1\u1090\'f2\u1072\'e0
\u1083\'eb\u1080\'e8 \u1080\'e8\u1079\'e7 \u1084\'ec\u1072\'e0\u1083\'eb\u1099\'fb\u1093\'f5 \u1084\'ec\u1072\'e0\u1089\'f1\u1096\'f8\u1090\'f2\u1072\'e0\u1073\'e1\u1086\'ee\u1074\'e2. \u1050\'ca\u1091\'f3\u1083\'eb\u1100\'fc
\u1090\'f2\u1091\'f3\u1088\'f0\u1072\'e0 \u1080\'e8\u1090\'f2\u1072\'e0\u1083\'eb\u1100\'fc\u1103\'ff\u1085\'ed\u1089\'f1\u1082\'ea\u1086\'ee\u1075\'e3\u1086\'ee \u1056\'d0\u1077\'e5\u1085\'ed\u1077\'e5\u1089\'f1\u1089\'f1
\u1072\'e0\u1085\'ed\u1089\'f1\u1072\'e0 \u1073\'e1\u1099\'fb\u1083\'eb\u1072\'e0 \u1088\'f0\u1077\'e5\u1075\'e3\u1080\'e8\u1086\'ee\u1085\'ed\u1072\'e0\u1083\'eb\u1100\'fc\u1085\'ed\u1086\'ee\u1081\'e9; \u1082\'ea\u1091\'f3
\u1083\'eb\u1100\'fc\u1090\'f2\u1091\'f3\u1088\'f0\u1072\'e0 \u1043\'c3\u1077\'e5\u1088\'f0\u1084\'ec\u1072\'e0\u1085\'ed\u1080\'e8\u1080\'e8 \u1101\'fd\u1087\'ef\u1086\'ee\u1093\'f5\u1080\'e8 \u1043\'c3\u1077\'e5\u1090\'f2
\u1077\'e5 \u1073\'e1\u1099\'fb\u1083\'eb\u1072\'e0 \u1088\'f0\u1077\'e5\u1075\'e3\u1080\'e8\u1086\'ee\u1085\'ed\u1072\'e0\u1083\'eb\u1100\'fc\u1085\'ed\u1086\'ee\u1081\'e9; \u1040\'c0\u1085\'ed\u1075\'e3\u1083\'eb\u1080\'e8
\u1103\'ff \u1076\'e4\u1086\'ee \u1089\'f1\u1080\'e8\u1093\'f5 \u1087\'ef\u1086\'ee\u1088\'f0 \u1086\'ee\u1089\'f1\u1090\'f2\u1072\'e0\u1077\'e5\u1090\'f2\u1089\'f1\u1103\'ff \u1082\'ea\u1083\'eb\u1072\'e0\u1089\'f1\u1089\'f1
\u1080\'e8\u1095\'f7\u1077\'e5\u1089\'f1\u1082\'ea\u1086\'ee\u1081\'e9 \u1089\'f1\u1090\'f2\u1088\'f0\u1072\'e0\u1085\'ed\u1086\'ee\u1081\'e9 \u1084\'ec\u1077\'e5\u1089\'f1\u1090\'f2\u1085\'ed\u1086\'ee\u1075\'e3\u1086\'ee
 \u1087\'ef\u1072\'e0\u1090\'f2\u1088\'f0\u1080\'e8\u1086\'ee\u1090\'f2\u1080\'e8\u1079\'e7\u1084\'ec\u1072\'e0. \u1052\'cc\u1086\'ee\u1078\'e6\u1077\'e5\u1090\'f2 \u1073\'e1\u1099\'fb\u1090\'f2\u1100\'fc, \u1082\'ea\u1086\'ee
\u1075\'e3\u1076\'e4\u1072\'e0-\u1085\'ed\u1080\'e8\u1073\'e1\u1091\'f3\u1076\'e4\u1100\'fc \u1091\'f3\u1076\'e4\u1072\'e0\u1089\'f1\u1090\'f2\u1089\'f1\u1103\'ff \u1076\'e4\u1086\'ee\u1082\'ea\u1072\'e0\u1079\'e7\u1072\'e0
\u1090\'f2\u1100\'fc, \u1095\'f7\u1090\'f2\u1086\'ee \u1082\'ea\u1091\'f3\u1083\'eb\u1100\'fc\u1090\'f2\u1091\'f3\u1088\'f0\u1099\'fb, \u1074\'e2\u1099\'fb\u1088\'f0\u1086\'ee\u1089\'f1\u1096\'f8\u1080\'e8\u1077\'e5 \u1085\'ed
\u1072\'e0 \u1087\'ef\u1086\'ee\u1095\'f7\u1074\'e2\u1077\'e5 \u1088\'f0\u1077\'e5\u1075\'e3\u1080\'e8\u1086\'ee\u1085\'ed\u1072\'e0\u1083\'eb\u1080\'e8\u1079\'e7\u1084\'ec\u1072\'e0, \u1089\'f1\u1091\'f3\u1097\'f9\u1077\'e5
\u1089\'f1\u1090\'f2\u1074\'e2\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee \u1086\'ee\u1090\'f2\u1083\'eb\u1080\'e8\u1095\'f7\u1072\'e0\u1102\'fe\u1090\'f2\u1089\'f1\u1103\'ff \u1086\'ee\u1090\'f2 \u1082\'ea\u1091\'f3\u1083\'eb
\u1100\'fc\u1090\'f2\u1091\'f3\u1088\'f0 \u1085\'ed\u1077\'e5\u1081\'e9\u1090\'f2\u1088\'f0\u1072\'e0\u1083\'eb\u1080\'e8\u1089\'f1\u1090\'f2\u1089\'f1\u1082\'ea\u1080\'e8\u1093\'f5 \u1080\'e8 \u1080\'e8\u1084\'ec\u1087\'ef
\u1077\'e5\u1088\'f0\u1089\'f1\u1082\'ea\u1080\'e8\u1093\'f5, \u1095\'f7\u1090\'f2\u1086\'ee \u1087\'ef\u1077\'e5\u1088\'f0\u1074\'e2\u1099\'fb\u1077\'e5 \u1073\'e1\u1099\'fb\u1083\'eb\u1080\'e8, \u1087\'ef\u1086\'ee\u1078\'e6
\u1072\'e0\u1083\'eb\u1091\'f3\u1081\'e9, \u1082\'ea\u1072\'e0\u1082\'ea-\u1090\'f2\u1086\'ee \u1089\'f1\u1086\'ee\u1095\'f7\u1085\'ed\u1077\'e5\u1077\'e5, \u1073\'e1\u1083\'eb\u1080\'e8\u1078\'e6\u1077\'e5 \u1082\'ea \u1079\'e7
\u1077\'e5\u1084\'ec\u1083\'eb\u1077\'e5 \u1080\'e8 \u1074\'e2\u1086\'ee\u1086\'ee\u1073\'e1\u1097\'f9\u1077\'e5 \u1073\'e1\u1086\'ee\u1083\'eb\u1077\'e5\u1077\'e5 \u1073\'e1\u1091\'f3\u1081\'e9\u1085\'ed\u1086\'ee \u1088\'f0
\u1072\'e0\u1079\'e7\u1088\'f0\u1072\'e0\u1089\'f1\u1090\'f2\u1072\'e0\u1083\'eb\u1080\'e8\u1089\'f1\u1100\'fc. \u1053\'cd\u1086\'ee \u1087\'ef\u1086\'ee\u1082\'ea\u1072\'e0 \u1101\'fd\u1090\'f2\u1086\'ee \u1085\'ed\u1077\'e5
 \u1076\'e4\u1086\'ee\u1082\'ea\u1072\'e0\u1079\'e7\u1072\'e0\u1085\'ed\u1086\'ee, \u1091\'f3\u1076\'e4\u1086\'ee\u1074\'e2\u1083\'eb\u1077\'e5\u1090\'f2\u1074\'e2\u1086\'ee\u1088\'f0\u1080\'e8\u1084\'ec\u1089\'f1\u1103\'ff
 \u1091\'f3\u1090\'f2\u1074\'e2\u1077\'e5\u1088\'f0\u1078\'e6\u1076\'e4\u1077\'e5\u1085\'ed\u1080\'e8\u1077\'e5\u1084\'ec, \u1095\'f7\u1090\'f2\u1086\'ee \u1087\'ef\u1086\'ee \u1082\'ea\u1088\'f0\u1072\'e0\u1081\'e9\u1085\'ed
\u1077\'e5\u1081\'e9 \u1084\'ec\u1077\'e5\u1088\'f0\u1077\'e5 \u1076\'e4\u1083\'eb\u1103\'ff \u1078\'e6\u1080\'e8\u1079\'e7\u1085\'ed\u1080\'e8 \u1074\'e2 \u1041\'c1\u1077\'e5\u1088\'f0\u1086\'ee\u1091\'f3\u1085\'ed\u1077\'e5
 \u1074\'e2\u1072\'e0\u1078\'e6\u1085\'ed\u1086\'ee, \u1095\'f7\u1090\'f2\u1086\'ee\u1073\'e1\u1099\'fb \u1101\'fd\u1090\'f2\u1086\'ee\u1090\'f2 \u1075\'e3\u1086\'ee\u1088\'f0\u1086\'ee\u1076\'e4 \u1074\'e2\u1084\'ec\u1077\'e5
\u1089\'f1\u1090\'f2\u1077\'e5 \u1089\'f1 \u1077\'e5\u1075\'e3\u1086\'ee \u1086\'ee\u1082\'ea\u1088\'f0\u1077\'e5\u1089\'f1\u1090\'f2\u1085\'ed\u1086\'ee\u1089\'f1\u1090\'f2\u1103\'ff\u1084\'ec\u1080\'e8 \u1095\'f7\u1091\'f3
\u1074\'e2\u1089\'f1\u1090\'f2\u1074\'e2\u1086\'ee\u1074\'e2\u1072\'e0\u1083\'eb \u1089\'f1\u1077\'e5\u1073\'e1\u1103\'ff \u1089\'f1\u1082\'ea\u1086\'ee\u1088\'f0\u1077\'e5\u1077\'e5 \u1089\'f1\u1072\'e0\u1084\'ec\u1086\'ee
\u1089\'f1\u1090\'f2\u1086\'ee\u1103\'ff\u1090\'f2\u1077\'e5\u1083\'eb\u1100\'fc\u1085\'ed\u1099\'fb\u1084\'ec \u1082\'ea\u1088\'f0\u1072\'e0\u1077\'e5\u1084\'ec, \u1095\'f7\u1077\'e5\u1084\'ec \u1087\'ef\u1088\'f0\u1086\'ee
\u1074\'e2\u1080\'e8\u1085\'ed\u1094\'f6\u1080\'e8\u1077\'e5\u1081\'e9. \u1044\'c4\u1083\'eb\u1103\'ff \u1084\'ec\u1086\'ee\u1088\'f0\u1072\'e0\u1083\'eb\u1100\'fc\u1085\'ed\u1086\'ee\u1075\'e3\u1086\'ee \u1089\'f1\u1086\'ee
\u1089\'f1\u1090\'f2\u1086\'ee\u1103\'ff\u1085\'ed\u1080\'e8\u1103\'ff \u1058\'d2\u1088\'f0\u1096\'f8\u1077\'e5\u1073\'e1\u1086\'ee\u1085\'ed\u1080\'e8 \u1087\'ef\u1086\'ee\u1083\'eb\u1077\'e5\u1079\'e7\u1085\'ed\u1086\'ee
 \u1086\'ee\u1089\'f1\u1086\'ee\u1079\'e7\u1085\'ed\u1072\'e0\u1090\'f2\u1100\'fc, \u1095\'f7\u1090\'f2\u1086\'ee \u1093\'f5\u1086\'ee\u1090\'f2\u1103\'ff \u1090\'f2\u1088\'f0\u1096\'f8\u1077\'e5\u1073\'e1\u1086\'ee\u1085\'ed
\u1100\'fc\u1089\'f1\u1082\'ea\u1072\'e0\u1103\'ff \u1087\'ef\u1083\'eb\u1086\'ee\u1097\'f9\u1072\'e0\u1076\'e4\u1100\'fc \u1085\'ed\u1077\'e5 \u1090\'f2\u1072\'e0\u1082\'ea \u1086\'ee\u1078\'e6\u1080\'e8\u1074\'e2\u1083\'eb
\u1077\'e5\u1085\'ed\u1072\'e0, \u1082\'ea\u1072\'e0\u1082\'ea \u1087\'ef\u1088\'f0\u1072\'e0\u1078\'e6\u1089\'f1\u1082\'ea\u1080\'e8\u1081\'e9 \u1087\'ef\u1077\'e5\u1088\'f0\u1077\'e5\u1082\'ea\u1088\'f0\u1077\'e5\u1089\'f1
\u1090\'f2\u1086\'ee\u1082\'ea \u1085\'ed\u1072\'e0 \u1052\'cc\u1091\'f3\u1089\'f1\u1090\'f2\u1082\'ea\u1091\'f3, \u1079\'e7\u1072\'e0\u1090\'f2\u1086\'ee \u1082\'ea\u1088\'f0\u1072\'e0\u1089\'f1\u1080\'e8\u1074\'e2\u1077\'e5
\u1077\'e5 \u1077\'e5\u1075\'e3\u1086\'ee, \u1086\'ee\u1089\'f1\u1086\'ee\u1073\'e1\u1077\'e5\u1085\'ed\u1085\'ed\u1086\'ee \u1077\'e5\u1089\'f1\u1083\'eb\u1080\'e8 \u1101\'fd\u1090\'f2\u1091\'f3 \u1087\'ef\u1083\'eb\u1086\'ee
\u1097\'f9\u1072\'e0\u1076\'e4\u1100\'fc \u1095\'f7\u1091\'f3\u1090\'f2\u1100\'fc \u1083\'eb\u1091\'f3\u1095\'f7\u1096\'f8\u1077\'e5 \u1079\'e7\u1072\'e0\u1084\'ec\u1086\'ee\u1089\'f1\u1090\'f2\u1080\'e8\u1090\'f2\u1100\'fc
. \u1041\'c1\u1099\'fb\u1083\'eb\u1086\'ee \u1073\'e1\u1099\'fb \u1086\'ee\u1095\'f7\u1077\'e5\u1085\'ed\u1100\'fc \u1074\'e2\u1072\'e0\u1078\'e6\u1085\'ed\u1086\'ee, \u1095\'f7\u1090\'f2\u1086\'ee\u1073\'e1\u1099\'fb \u1073\'e1
\u1088\'f0\u1072\'e0\u1090\'f2 \u1089\'f1\u1083\'eb\u1086\'ee\u1074\'e2\u1072\'e0\u1082\'ea \u1080\'e8\u1079\'e7 \u1053\'cd\u1080\'e8\u1090\'f2\u1088\'f0\u1099\'fb \u1073\'e1\u1086\'ee\u1083\'eb\u1100\'fc\u1096\'f8\u1077\'e5
 \u1076\'e4\u1091\'f3\u1084\'ec\u1072\'e0\u1083\'eb \u1086\'ee \u1073\'e1\u1083\'eb\u1072\'e0\u1075\'e3\u1086\'ee\u1091\'f3\u1089\'f1\u1090\'f2\u1088\'f0\u1086\'ee\u1081\'e9\u1089\'f1\u1090\'f2\u1074\'e2\u1077\'e5 \u1075\'e3
\u1086\'ee\u1088\'f0\u1086\'ee\u1076\'e4\u1072\'e0 \u1053\'cd\u1080\'e8\u1090\'f2\u1088\'f0\u1072\'e0, \u1095\'f7\u1077\'e5\u1084\'ec \u1086\'ee\u1073\'e1 \u1072\'e0\u1074\'e2\u1090\'f2\u1086\'ee\u1085\'ed\u1086\'ee\u1084\'ec
\u1080\'e8\u1080\'e8 \u1073\'e1\u1088\'f0\u1072\'e0\u1090\'f2\u1080\'e8\u1089\'f1\u1083\'eb\u1072\'e0\u1074\'e2\u1089\'f1\u1082\'ea\u1080\'e8\u1093\'f5 \u1095\'f7\u1080\'e8\u1085\'ed\u1086\'ee\u1074\'e2\u1085\'ed\u1080\'e8
\u1082\'ea\u1086\'ee\u1074\'e2. \u1047\'c7\u1085\'ed\u1072\'e0\u1095\'f7\u1080\'e8\u1090\'f2\u1077\'e5\u1083\'eb\u1100\'fc\u1085\'ed\u1086\'ee \u1074\'e2\u1072\'e0\u1078\'e6\u1085\'ed\u1077\'e5\u1077\'e5, \u1095\'f7\u1090\'f2
\u1086\'ee\u1073\'e1\u1099\'fb \u1075\'e3\u1086\'ee\u1088\'f0\u1086\'ee\u1076\'e4 \u1063\'d7\u1072\'e0\u1089\'f1\u1083\'eb\u1072\'e0\u1074\'e2 \u1073\'e1\u1099\'fb\u1083\'eb \u1073\'e1\u1072\'e0\u1089\'f1\u1090\'f2\u1080\'e8
\u1086\'ee\u1085\'ed\u1086\'ee\u1084\'ec \u1095\'f7\u1072\'e0\u1089\'f1\u1083\'eb\u1072\'e0\u1074\'e2\u1089\'f1\u1082\'ea\u1086\'ee\u1075\'e3\u1086\'ee \u1082\'ea\u1088\'f0\u1072\'e0\u1103\'ff, \u1095\'f7\u1077\'e5\u1084\'ec
 \u1073\'e1\u1072\'e0\u1089\'f1\u1090\'f2\u1080\'e8\u1086\'ee\u1085\'ed\u1086\'ee\u1084\'ec \u1090\'f2\u1086\'ee\u1081\'e9 \u1080\'e8\u1083\'eb\u1080\'e8 \u1080\'e8\u1085\'ed\u1086\'ee\u1081\'e9 \u1087\'ef\u1086\'ee\u1083\'eb
\u1080\'e8\u1090\'f2\u1080\'e8\u1095\'f7\u1077\'e5\u1089\'f1\u1082\'ea\u1086\'ee\u1081\'e9 \u1087\'ef\u1072\'e0\u1088\'f0\u1090\'f2\u1080\'e8\u1080\'e8. \u1071\'df \u1074\'e2 \u1089\'f1\u1072\'e0\u1084\'ec\u1086\'ee\u1084\'ec
 \u1076\'e4\u1077\'e5\u1083\'eb\u1077\'e5 \u1089\'f1\u1095\'f7\u1080\'e8\u1090\'f2\u1072\'e0\u1102\'fe, \u1095\'f7\u1090\'f2\u1086\'ee \u1089\'f1\u1077\'e5\u1081\'e9\u1095\'f7\u1072\'e0\u1089\'f1 \u1085\'ed\u1072\'e0\u1084\'ec
 \u1073\'e1\u1086\'ee\u1083\'eb\u1077\'e5\u1077\'e5 \u1074\'e2\u1089\'f1\u1077\'e5\u1075\'e3\u1086\'ee \u1085\'ed\u1091\'f3\u1078\'e6\u1077\'e5\u1085\'ed \u1073\'e1\u1086\'ee\u1075\'e3\u1072\'e0\u1090\'f2\u1086\'ee \u1088\'f0
\u1072\'e0\u1079\'e7\u1074\'e2\u1077\'e5\u1090\'f2\u1074\'e2\u1083\'eb\u1077\'e5\u1085\'ed\u1085\'ed\u1099\'fb\u1081\'e9 \u1088\'f0\u1077\'e5\u1075\'e3\u1080\'e8\u1086\'ee\u1085\'ed\u1072\'e0\u1083\'eb\u1080\'e8\u1079\'e7
\u1084\'ec.\par
